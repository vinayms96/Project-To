<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" verbose="2" parallel="none">
	<listeners>
		<listener class-name="modules.TestListners"></listener>
	</listeners>

	<test thread-count="5" name="Chrome">
		<parameter name="browser" value="Chrome"></parameter>
		<classes>
			<class name="modules.projectSetup" />
			<class name="testCases.loginFeature" />
			<class name="testCases.verifyLinks" />
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="Firefox">
		<parameter name="browser" value="Firefox"></parameter>
		<classes>
			<class name="modules.projectSetup" />
			<class name="testCases.loginFeature" />
			<class name="testCases.verifyLinks" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
